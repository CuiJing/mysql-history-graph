        {rank=same; "2007" "MySQL Cluster 6.2" }
        {rank=same; "2008" "MySQL 5.1" "MySQL Cluster 6.3" }
        {rank=same; "2009" "MySQL 6.0" "MySQL 5.2" "MySQL 5.4" "Percona Server 5.1" "MySQL Cluster 7.0"}
